<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="KlarnaPayment\Components\DataAbstractionLayer\Entity\RequestLog\RequestLogDefinition">
            <tag name="shopware.entity.definition" entity="klarna_payment_request_log"/>
        </service>

        <service id="KlarnaPayment\Components\DataAbstractionLayer\Entity\Order\OrderExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="KlarnaPayment\Components\DataAbstractionLayer\Entity\Order\OrderExtensionDefinition">
            <tag name="shopware.entity.definition"/>
        </service>

        <service id="KlarnaPayment\Components\DataAbstractionLayer\Entity\Cart\KlarnaCartDefinition">
            <tag name="shopware.entity.definition" entity="klarna_cart"/>
        </service>
    </services>
</container>
